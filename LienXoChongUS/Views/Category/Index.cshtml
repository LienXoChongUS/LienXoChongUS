@model List<Category>
<h1>Category List</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Display Order</th>
        </tr>
    </thead>
    <tbody>
     
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))
        {
            <tr>
                <td>@obj.Id</td>
                <td>@obj.Name</td>
                <td>@obj.Description</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
       
    </tbody>
</table>